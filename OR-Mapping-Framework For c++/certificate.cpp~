#include"certificate.h"
#include<iostream>
#include<sstream>
#include<string>
using namespace std;
extern  int cid;
extern	string certificate_name;
extern	int employee_id;
extern  *employee;

string Certificate::serialize(){
	ostringstream oss;
	oss << cid;
	string s = oss.str();
	return (s+","+certificaate_name+","+employee_id);

}
Certificate::Certificate(int cid,string certificate_name,int employee_id){

	this->cid=cid;
	this->certificate_name=certificate_name;
	this->employee_id=employee_id;

}
